# generated by ovirt-hosted-engine-setup
BOOTPROTO=none
DEVICE=eth0
HWADDR={{ VM_MAC_ADDR }}
ONBOOT=yes
TYPE=Ethernet
USERCTL=no
ZONE=public
IPADDR={{ VM_IP_ADDR }}
PREFIX={{ VM_IP_PREFIX }}
GATEWAY={{ GATEWAY }}
{% if DNS_ADDR is string %}
{% set DNS_ADDR_LIST = DNS_ADDR.split(',') %}
{% elif DNS_ADDR is iterable %}
{% set DNS_ADDR_LIST = DNS_ADDR %}
{% else %}
{% set DNS_ADDR_LIST = [] %}
{% endif %}
{% for d in DNS_ADDR_LIST %}
DNS{{loop.index}}={{ d }}
{% endfor %}
DEFROUTE=yes
IPV4_FAILURE_FATAL=no
IPV6INIT=no
NM_CONTROLLED=no